@model List<BootStrapExample.Models.Character>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="jumbotron">
    <h1>TESTING</h1>
    <p>I NEED THIS WORKING, PLEASE WORK</p>
    <div class="row">
        @foreach (var item in Model)
        {
            <div class="col-sm-6 col-md-4">
                <div class="thumbnail" style="padding: 20px;">
                    <img src="~/Images/@(item.Name).png" alt="@item.Name">
                    <div class="caption">
                        <h3>@item.Name</h3>
                        <p>@item.Summary</p>
                    </div>
                </div>
            </div>
        }
     </div>
</div>

